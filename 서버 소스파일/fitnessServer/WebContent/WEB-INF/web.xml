<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.4">
  <display-name>fitnessServer</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>Membership</servlet-name>
  	<servlet-class>member.Membership</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>Membership</servlet-name>
  	<url-pattern>/tryLogin</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>Membership</servlet-name>
  	<url-pattern>/ShowAllMemberList</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>Membership</servlet-name>
  	<url-pattern>/SearchMemberList</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>Membership</servlet-name>
  	<url-pattern>/InsertMember</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>Membership</servlet-name>
  	<url-pattern>/UpdateMember</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>Membership</servlet-name>
  	<url-pattern>/DeleteMember</url-pattern>
  </servlet-mapping>
</web-app>